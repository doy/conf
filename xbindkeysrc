"urxvtc"
  Mod4 + o

"firefox"
  Mod4 + b

"dmenu -fn '7x14' -p 'pass search' < /dev/null | xargs -I{} pass search -c {}"
  Mod4 + i

"dmenu -fn '7x14' -p 'pass search -u' < /dev/null | xargs -I{} pass search -u -c {}"
  Shift+Mod4 + i

"dmenu_run -fn '7x14'"
  Mod4 + r

"screenshot"
  Mod4 + s

"screenshot --full"
  Shift+Mod4 + s

"xscreensaver-command -lock"
  Mod4 + d

"toggle_numkeys"
  Shift + Shift_R

"toggle_numkeys"
  Shift + Shift_L

"volume up"
  XF86AudioRaiseVolume

"volume down"
  XF86AudioLowerVolume

"volume mute"
  XF86AudioMute

"pactl set-source-mute 2 toggle"
  XF86AudioMicMute

"xrandr --auto"
  XF86Display

"xset dpms force off"
  XF86Search

"kill_focused STOP"
  XF86LaunchA

"kill_focused CONT"
  XF86Explorer

"xbacklight -inc 10"
  XF86MonBrightnessUp

"xbacklight -dec 10"
  XF86MonBrightnessDown
