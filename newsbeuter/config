error-log "~/.newsbeuter/error.log"

auto-reload yes
reload-threads 4
suppress-first-reload yes
show-read-feeds no

browser firefox

color listfocus black cyan
color info      green blue bold

bind-key j down
bind-key k up
bind-key i quit
unbind-key i feedlist

macro w set-tag webcomics
macro b set-tag blog

ignore-article "http://drmcninja.com/feed" "title !~ \"[[:digit:]]+p[[:digit:]]+\""
ignore-article "http://rsspect.com/rss/asw.xml" "title !~ \"A Softer World: [[:digit:]]\""
